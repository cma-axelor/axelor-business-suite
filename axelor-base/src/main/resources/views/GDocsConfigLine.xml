<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<grid name="gdocs-config-line-grid" title="GDocs config lines" model="com.axelor.apps.base.db.GDocsConfigLine">
		<field name="code"/>
		<field name="metaModel"/>
	</grid>

	<form name="gdocs-config-line-form" title="GDocs config line" model="com.axelor.apps.base.db.GDocsConfigLine" width="large">
		<panel name="mainPanel">
			<field name="code"/>
			<spacer colSpan="6"/>
			<field name="metaModel"/>
			<field name="destinationFolderID"/>
			<field name="templateList" colSpan="12">
				<editor x-show-titles="false">
					<field name="targetId" colSpan="12"/>
				</editor>
			</field>
		</panel>
	</form>
	
	

</object-views>